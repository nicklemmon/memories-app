{
  "name": "memories-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/react": "^9.4.0",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "history": "^4.7.2",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "parse": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.12.0",
    "react-focus-lock": "^1.19.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.0.5",
    "react-scripts": "3.3.0",
    "reset-css": "^3.0.0",
    "react-router-dom": "^5.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:ci": "http-server build -p 3000 -c-1 --silent --cors",
    "start-dev": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "test:cy:run": "$(npm bin)/cypress run",
    "test:cy:open": "$(npm bin)/cypress open",
    "test:cy:ci": "npm start & wait-on http://localhost:3000 && npm run test:cy:run"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "cypress": "^3.8.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-testing-library": "^1.3.4",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.8.3",
    "wait-on": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
